                  <Insets right="20.0" />
               </padding>
            </Label>
            <Label fx:id="EventStartTimeLabel" layoutX="339.0" layoutY="13.0" text="Start Time" AnchorPane.leftAnchor="339.0">
               <padding>
                  <Insets right="20.0" />
               </padding>
            </Label>
            <Label fx:id="EventEndTimeLabel" layoutX="486.0" layoutY="13.0" text="End Time" AnchorPane.leftAnchor="486.0">
               <padding>
                  <Insets right="20.0" />
               </padding>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="EventNameButton" layoutX="51.0" layoutY="204.0" mnemonicParsing="false" onAction="#changeNameButton" text="Change" />
      <Button fx:id="EventDateButton" layoutX="200.0" layoutY="204.0" mnemonicParsing="false" onAction="#dateChangeButton" text="Change" />
      <Button fx:id="EventStartTimeButton" layoutX="339.0" layoutY="204.0" mnemonicParsing="false" onAction="#startTimeChangeButton" text="Change" />
      <Button fx:id="EventEndTimeButton" layoutX="486.0" layoutY="204.0" mnemonicParsing="false" onAction="#endTimeChangeButton" text="Change" />
      <Label fx:id="statusLabel" layoutX="449.0" layoutY="256.0" text="Status" AnchorPane.bottomAnchor="27.0" AnchorPane.rightAnchor="13.5" AnchorPane.topAnchor="256.0">
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </Label>
